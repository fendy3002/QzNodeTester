{
  "name": "qz-node-tester-express",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "run-p server:dev react:compile-dev css:compile-dev",
    "server:dev": "nodemon",
    "react:compile-dev": "webpack --mode=development --config webpack.dev.js --watch",
    "css": "npm-run-all css:compile css:prefix css:minify",
    "css:compile-dev": "sass --style expanded --source-map --embed-sources --no-error-css --watch scss/index.scss:public/css/compiled.css",
    "css:compile": "sass --style expanded --source-map --embed-sources --no-error-css scss/index.scss:public/css/compiled.css",
    "css:prefix": "postcss --config scss/bootstrap/postcss.config.js --replace \"public/css/compiled.css\"",
    "css:minify": "cleancss -O1 --format breakWith=lf --with-rebase --source-map --source-map-inline-sources --output public/css --batch --batch-suffix \".min\" \"public/css/*.css\" \"!public/css/*.min.css\""
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.8",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@fendy3002/express-helper": "^0.4.1",
    "@fendy3002/qz-node": "^2.5.2",
    "@fendy3002/qz-webform": "^0.1.19",
    "@fendy3002/scaffolder": "^0.3.26",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.6",
    "babel-loader": "^8.2.3",
    "change-case": "^4.1.2",
    "connect-flash": "^0.1.1",
    "connect-redis": "^6.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "css-loader": "^6.5.0",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "history": "5",
    "lodash": "^4.17.21",
    "markdown-it": "^12.2.0",
    "mobx": "^6.3.6",
    "mobx-react": "^7.2.1",
    "mocha": "^9.1.3",
    "mysql2": "^2.3.2",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6",
    "redis": "^3.1.2",
    "sass": "^1.48.0",
    "sass-loader": "^12.3.0",
    "sequelize": "^6.8.0",
    "style-loader": "^3.3.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "url-loader": "^4.1.1",
    "uuid": "^8.3.2",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  }
}
